
<tr id="post-<%= post.id %>">
	<td>
		<table class="post info">
			<tr><td><%= link_to "#{post.user.username}", post.user %></td></tr>
			<tr><td>AVATAR</td></tr>
			<tr><td><%= post.id %></td></tr>
		</table>
	</td>

	<td>
		<table class="post content">
			<tr><td>Upper Content</td></tr>
			<tr><td><%= post.content %></td></tr>
			<tr><td>Lower Content</td></tr>
		</table>
	</td>
</tr>

