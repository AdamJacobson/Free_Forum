<% provide(:title, "Edit user") %>
<h1>Update your profile</h1>

<div >
  <%= form_for(@user) do |f| %>
  <%= render 'shared/error_messages', object: f.object %>
  <br/>
  <%= f.label :email %>
  <%= f.text_field :email %>
  <br/>
  <%= f.submit "Save changes", class: "btn btn-large btn-primary" %>
  <% end %>
</div>